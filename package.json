{
  "name": "DDEV-UI",
  "description": "A GUI to compliment the DDEV CLI",
  "version": "0.2.0",
  "main": "main.js",
  "scripts": {
    "start": "electron .",
    "build-linux": "electron-packager . --overwrite --platform=linux --arch=x64 --icon=build/icon.png --prune=true --out=release-builds --electronVersion=1.7.6",
    "build-win": "electron-packager . --overwrite --asar=true --platform=win32 --arch=ia32 --icon=build/icon.ico --prune=true --out=release-builds --version-string.CompanyName=CE --version-string.FileDescription=CE --version-string.ProductName='DDEV-UI' --electronVersion=1.7.6",
    "build-osx": "electron-packager . --overwrite --platform=darwin --arch=x64 --icon=build/icon.icns --prune=true --out=release-builds --electronVersion=1.7.6"
  },
  "author": "Andrew C Tran",
  "devDependencies": {
    "electron-packager": "^9.1.0"
  },
  "dependencies": {
    "fix-path": "^2.1.0",
    "tarball-extract": "0.0.6"
  }
}
