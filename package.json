{
  "name": "ddev-ui",
  "description": "A GUI to complement the DDEV CLI",
  "version": "v0.4.0-alpha",
  "main": "src/main/index.js",
  "homepage": "https://github.com/drud/ddev",
  "author": {
    "name": "DRUD Tech",
    "email": "support@drud.com"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "scripts": {
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "lint:tests": "eslint --cache --color --ext .jsx,.js test",
    "build-linux": "yarn compile && electron-builder --linux --x64",
    "build-windows": "yarn compile && electron-builder --win",
    "build-darwin": "yarn compile && electron-builder --mac",
    "test": "mocha --require babel-register --require ignore-styles",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "DDEV UI",
    "appId": "ddev-ui",
    "dmg": {
      "window": {
        "width": 600,
        "height": 360
      },
      "contents": [
        {
          "x": 150,
          "y": 135
        },
        {
          "x": 450,
          "y": 135,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "development"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.config.js"
    },
    "main": {
      "webpackConfig": "webpack.main.config.js"
    }
  },
  "dependencies": {
    "app-root-path": "2.1.x",
    "babel-loader": "^8.0.0-beta",
    "bootstrap": "4.1.x",
    "compare-versions": "3.x",
    "electron-devtools-installer": "2.2.x",
    "electron-is-dev": "0.3.x",
    "fix-path": "2.1.x",
    "font-awesome": "4.7.x",
    "jquery": "3.3.x",
    "lodash": "4.17.x",
    "popper.js": "1.14.x",
    "prop-types": "15.x",
    "react": "16.x",
    "react-data-grid": "4.x",
    "react-desktop": "0.3.x",
    "react-dom": "16.x",
    "react-hot-loader": "4.x",
    "react-router": "4.x",
    "react-router-dom": "4.x",
    "request": "2.x",
    "source-map-support": "0.5.x",
    "sudo-prompt": "8.x",
    "tar": "4.x",
    "xml2js": "0.4.x"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-rc.3",
    "@babel/node": "^7.0.0-rc.3",
    "@babel/plugin-proposal-class-properties": "^7.0.0-rc.3",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-rc.3",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-rc.3",
    "@babel/preset-env": "^7.0.0-rc.3",
    "@babel/preset-react": "^7.0.0-rc.3",
    "@babel/register": "^7.0.0-rc.3",
    "babel-eslint": "^v9.0.0-beta.3",
    "babel-plugin-root-import": "6.x",
    "cypress": "3.x",
    "electron": "2.x",
    "electron-builder": "20.x",
    "electron-webpack": "2.1.x",
    "eslint": "4.x",
    "eslint-config-airbnb": "17.x",
    "eslint-config-prettier": "3.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "6.x",
    "eslint-plugin-prettier": "2.x",
    "eslint-plugin-react": "7.x",
    "file-loader": "1.x",
    "ignore-styles": "5.x",
    "jest": "23.x",
    "mocha": "5.x",
    "mock-fs": "4.x",
    "nock": "9.x",
    "node-sass": "4.x",
    "prettier": "1.x",
    "rewire": "4.x",
    "sass-loader": "7.x",
    "stub-spawn-once": "2.3.x",
    "webpack": "4.x"
  },
  "resolutions": {
    "webpack-sources": "1.x"
  }
}
