{
  "name": "ddev-ui",
  "description": "A GUI to complement the DDEV CLI",
  "version": "v0.4.0",
  "main": "src/main/index.js",
  "homepage": "https://github.com/drud/ddev",
  "author": {
    "name": "DRUD Tech",
    "email": "support@drud.com"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "scripts": {
    "start": "electron-webpack dev",
    "compile": "electron-webpack",
    "dist": "yarn compile && electron-builder",
    "dist:dir": "yarn dist --dir -c.compression=store -c.mac.identity=null",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "lint:tests": "eslint --cache --color --ext .jsx,.js test",
    "build-linux": "yarn compile && electron-builder --linux --x64",
    "build-windows": "yarn compile && electron-builder --win",
    "build-darwin": "yarn compile && electron-builder --mac",
    "test": "mocha --require babel-register --require ignore-styles",
    "postinstall": "electron-builder install-app-deps"
  },
  "build": {
    "productName": "DDEV UI",
    "appId": "ddev-ui",
    "dmg": {
      "window": {
        "width": 600,
        "height": 360
      },
      "contents": [
        {
          "x": 150,
          "y": 135
        },
        {
          "x": 450,
          "y": 135,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "development"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "electronWebpack": {
    "renderer": {
      "webpackConfig": "webpack.renderer.config.js"
    },
    "main": {
      "webpackConfig": "webpack.main.config.js"
    }
  },
  "dependencies": {
    "app-root-path": "2.1.x",
    "babel-loader": "^8.0.0-beta",
    "bootstrap": "4.1.x",
    "compare-versions": "3.1.x",
    "electron-devtools-installer": "2.2.x",
    "electron-is-dev": "0.3.x",
    "fix-path": "2.1.x",
    "font-awesome": "4.7.x",
    "jquery": "3.3.x",
    "lodash": "^4.17.10",
    "popper.js": "1.14.x",
    "prop-types": "15.6.x",
    "react": "16.4.x",
    "react-data-grid": "^4.0.7",
    "react-desktop": "0.3.x",
    "react-dom": "16.4.x",
    "react-hot-loader": "4.3.x",
    "react-router": "^4.3.1",
    "react-router-dom": "4.3.x",
    "request": "2.x",
    "source-map-support": "0.5.x",
    "sudo-prompt": "8.x",
    "tar": "4.4.x",
    "xml2js": "0.4.x"
  },
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.56",
    "@babel/node": "^7.0.0-beta.56",
    "@babel/plugin-proposal-class-properties": "^7.0.0-beta.56",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0-beta.56",
    "@babel/plugin-syntax-dynamic-import": "^7.0.0-beta.56",
    "@babel/preset-react": "^7.0.0-beta.56",
    "@babel/register": "^7.0.0-beta.56",
    "babel-eslint": "^8.2.6",
    "babel-plugin-root-import": "^6.1.0",
    "cypress": "^3.0.3",
    "electron": "1.7.x",
    "electron-builder": "20.x",
    "electron-webpack": "^2.x",
    "eslint": "^4",
    "eslint-config-airbnb": "^17.0.0",
    "eslint-config-prettier": "^2.9.0",
    "eslint-plugin-import": "^2",
    "eslint-plugin-jsx-a11y": "^6",
    "eslint-plugin-prettier": "^2.6.2",
    "eslint-plugin-react": "^7",
    "file-loader": "1.x",
    "ignore-styles": "^5.0.1",
    "jest": "^23.4.2",
    "mocha": "5.x",
    "mock-fs": "4.x",
    "nock": "9.x",
    "node-sass": "^4.9.2",
    "prettier": "^1.14.0",
    "rewire": "3.x",
    "sass-loader": "^7.0.3",
    "stub-spawn-once": "2.3.x",
    "webpack": "4.x"
  },
  "resolutions": {
    "webpack-sources": "1.x"
  }
}
