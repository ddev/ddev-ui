{
  "name": "ddev-ui",
  "description": "A GUI to complement the DDEV CLI",
  "version": "v0.3.0-dev",
  "main": "src/main/index.js",
  "homepage": "https://github.com/drud/ddev",
  "author": {
    "name": "DRUD Tech",
    "email": "support@drud.com"
  },
  "license": "Apache-2.0",
  "licenses": [
    {
      "type": "Apache-2.0",
      "url": "http://www.apache.org/licenses/LICENSE-2.0"
    }
  ],
  "scripts": {
    "start": "electron-forge start",
    "package": "electron-forge package",
    "make": "electron-forge make",
    "publish": "electron-forge publish",
    "lint": "eslint --cache --color --ext .jsx,.js src",
    "build-linux": "electron-builder --linux --x64",
    "build-windows": "electron-builder --win",
    "build-darwin": "electron-builder --mac",
    "test": "mocha --require babel-register --require ignore-styles"
  },
  "build": {
    "appId": "ddev-ui",
    "dmg": {
      "window": {
        "width": 600,
        "height": 360
      },
      "contents": [
        {
          "x": 150,
          "y": 135
        },
        {
          "x": 450,
          "y": 135,
          "type": "link",
          "path": "/Applications"
        }
      ]
    },
    "linux": {
      "target": [
        "AppImage"
      ],
      "category": "development"
    },
    "win": {
      "target": "nsis",
      "icon": "build/icon.ico"
    }
  },
  "config": {
    "forge": {
      "make_targets": {
        "win32": [
          "squirrel"
        ],
        "darwin": [
          "zip"
        ],
        "linux": [
          "deb",
          "rpm"
        ]
      },
      "electronPackagerConfig": {
        "packageManager": "yarn"
      },
      "electronWinstallerConfig": {
        "name": "ddev-ui"
      },
      "electronInstallerDebian": {},
      "electronInstallerRedhat": {},
      "github_repository": {
        "owner": "",
        "name": ""
      },
      "windowsStoreConfig": {
        "packageName": "",
        "name": "ddev-ui"
      }
    }
  },
  "dependencies": {
    "app-root-path": "2.1.x",
    "bootstrap": "4.1.x",
    "compare-versions": "3.1.x",
    "electron-devtools-installer": "2.1.x",
    "electron-is-dev": "0.3.x",
    "electron-squirrel-startup": "1.0.x",
    "fix-path": "2.1.x",
    "font-awesome": "4.7.x",
    "jquery": "3.3.x",
    "popper.js": "1.14.x",
    "prop-types": "15.6.x",
    "react": "15.6.x",
    "react-desktop": "0.3.x",
    "react-dom": "15.6.x",
    "react-hot-loader": "4.1.x",
    "react-router-dom": "4.3.x",
    "request": "2.x",
    "sudo-prompt": "8.x",
    "tar": "4.4.x",
    "tarball-extract": "0.0.x",
    "xml2js": "0.4.x"
  },
  "devDependencies": {
    "babel-plugin-transform-async-to-generator": "6.24.x",
    "babel-plugin-transform-class-properties": "6.24.x",
    "babel-plugin-transform-es2015-classes": "6.24.x",
    "babel-preset-env": "1.6.x",
    "babel-preset-react": "6.24.x",
    "cross-env": "5.1.x",
    "electron-builder": "20.x",
    "electron-forge": "5.x",
    "electron-prebuilt-compile": "1.x",
    "eslint": "3.x",
    "eslint-config-airbnb": "15.x",
    "eslint-plugin-import": "2.x",
    "eslint-plugin-jsx-a11y": "5.x",
    "eslint-plugin-react": "7.x",
    "file-loader": "1.1.x",
    "ignore-styles": "5.0.x",
    "mocha": "4.x",
    "mock-fs": "4.x",
    "nock": "9.x",
    "node-sass": "4.9.x",
    "rewire": "3.x",
    "sass-loader": "7.0.x",
    "spawn-sync": "2.0.x",
    "stub-spawn-once": "2.3.x",
    "webpack": "4.x"
  }
}
