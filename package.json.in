{
  "name": "ddev-ui",
  "description": "A GUI to complement the DDEV CLI",
  "version": "$DDEV_UI_VERSION",
  "main": "main.js",
  "scripts": {
    "test": "mocha",
    "build-linux": "electron-builder --linux --x64",
    "build-windows": "electron-builder --win",
    "build-darwin": "electron-builder --mac",
    "start": "NODE_ENV=DEV concurrently \"babel-node server.js\" \"sleep 2 && electron .\"",
    "bundle": "webpack",
    "watch": "NODE_ENV=DEV babel-node server.js",
    "start:prod": "webpack && electron ."
  },
  "author": "drudtech",
  "devDependencies": {
    "electron-builder": "^$ELECTRON_BUILDER_VERSION",
    "babel-cli": "^6.26.0",
    "babel-core": "^6.26.0",
    "babel-eslint": "^7.2.3",
    "babel-loader": "^7.1.2",
    "babel-polyfill": "^6.26.0",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-react-hmre": "^1.1.1",
    "concurrently": "^3.5.0",
    "electron-builder": "^19.33.0",
    "eslint": "^4.10.0",
    "eslint-config-airbnb": "^16.1.0",
    "eslint-loader": "^1.9.0",
    "eslint-plugin-import": "^2.8.0",
    "eslint-plugin-jsx-a11y": "^6.0.2",
    "eslint-plugin-react": "^7.4.0",
    "express": "^4.16.2",
    "mocha": "^4.0.1",
    "stub-spawn-once": "^2.3.0",
    "webpack": "^3.8.1",
    "webpack-dev-middleware": "^1.12.0",
    "webpack-hot-middleware": "^2.20.0"
  },
  "dependencies": {
    "fix-path": "^2.1.0",
    "tarball-extract": "0.0.6"
  }
}